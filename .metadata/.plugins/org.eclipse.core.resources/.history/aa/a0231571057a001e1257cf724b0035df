package com.cropDetails.Invoice.ExceptionsTest;

import static org.junit.jupiter.api.Assertions.assertThrows;
import org.junit.jupiter.api.Test;
import com.cropDetails.Invoice.Exceptions.NotFarmerException;

public class NotFarmerExceptionTest {

    @Test
    public void testNotFarmerException() {
        // Simulate a situation where the exception should be thrown
        assertThrows(NotFarmerException.class, () -> {
            throw new NotFarmerException();
        });
    }
}

