package com.cropDetails.Testing;

import static org.junit.jupiter.api.Assertions.assertThrows;

import org.junit.jupiter.api.Test;

import com.cropDetails.Exceptions.NotDealerException;

public class NotDealerExceptionTest {

    @Test
    public void testNotDealerException() {
        // Simulate a situation where the exception should be thrown
        assertThrows(NotDealerException.class, () -> {
            throw new NotDealerException();
        });
    }

    
}
